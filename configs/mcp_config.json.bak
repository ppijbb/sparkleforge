{
  "mcpServers": {
    "ddg_search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-duckduckgo-search@latest"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "fetch-mcp@latest"
      ]
    },
    "docfork": {
      "command": "npx",
      "args": [
        "-y",
        "docfork@latest"
      ]
    },
    "context7-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "exa": {
      "type": "http",
      "transport": "stdio",
      "httpUrl": "https://mcp.exa.ai/mcp",
      "headers": {
        "Remove-Me": "Disable web_search_exa tool if you're just coding. To 100% call exa-code, say 'use exa-code'."
      }
    },
    "semantic_scholar": {
      "command": "npx",
      "args": [
        "-y",
        "@hamid-vakilzadeh/mcpsemanticscholar@latest"
      ],
      "disabled": true
    },
    "parallel-search": {
      "command": "npx",
      "args": [
        "-y",
        "@parallel/search@latest"
      ],
      "disabled": true
    },
    "tavily-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@Jeetanshu18/tavily-mcp@latest"
      ],
      "disabled": true
    },
    "WebSearch-MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@mnhlt/WebSearch-MCP@latest"
      ],
      "disabled": true
    },
    "Google search console": {
      "command": "npx",
      "args": [
        "-y",
        "google_search_console@latest"
      ],
      "disabled": true
    },
    "Paper Search": {
      "command": "npx",
      "args": [
        "-y",
        "@adamamer20/paper-search-mcp-openai@latest"
      ],
      "disabled": true
    },
    "TodoList": {
      "command": "npx",
      "args": [
        "-y",
        "todoist@latest"
      ],
      "disabled": true
    },
    "arxiv": {
      "command": "npx",
      "transport": "stdio",
      "args": [
        "-y",
        "@modelcontextprotocol/server-arxiv@latest"
      ],
      "disabled": true
    },
    "kis MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@KISOpenAPI/kis-code-assistant-mcp@latest"
      ],
      "disabled": true
    },
    "financial_agent": {
      "command": "python",
      "args": [
        "lang_graph/financial_agent/financial_mcp_server.py"
      ],
      "disabled": true
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github@latest"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    }
  }
}

